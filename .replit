language = "python3"
run = "streamlit run app.py --server.port 8080 --server.address 0.0.0.0 --server.headless true --server.enableCORS false"

[nix]
channel = "stable-22_11"

[deployment]
run = ["sh", "-c", "streamlit run app.py --server.port 8080 --server.address 0.0.0.0 --server.headless true --server.enableCORS false"]
deploymentTarget = "cloudrun"
ignorePorts = false

[env]
PYTHONPATH = "/home/runner/$REPL_SLUG"
PORT = "8080"
